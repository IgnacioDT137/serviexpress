<%- include('partials/_header') -%>

<% if (typeof errorCrearUser != 'undefined') { %>
  <script>
    Swal.fire({
      icon: "error",
      title: "Error al ingresar usuario, verifique que no hayan datos repetidos",
      timer: 4000,
      showConfirmButton: false
    }).then(() => {
      window.location = "/admin/crudUsuarios"
    })
  </script>
<% } %>

